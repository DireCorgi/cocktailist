<div class="left wide">
  <% feedItems.each(function (feedItem){ %>
    <%= JST['feed/_feed']({feedItem: feedItem}) %>
  <% }); %>
</div>

<div class="rule-right"></div>

<div class="right bar">
  <div class="box">
    <h1><a href="javascript:void(0);" class="addEntry">Add Entry</a></h1>
  </div>
  <div class="box">
    <h1>My Lists</h1>
    <p>(placeholder for my lists subview)</p>
  </div>
  <div class="box">
    <h1>Today's Drink</h1>
    <% if(randomCocktail){ %>
    <ul class="category-show-list clearfix">
      <%= JST['cocktails/_categoryShowItem']({cocktail: randomCocktail, standalone: true}) %>
    </ul>
    <p><a href="#cocktails/browse/<%= randomCocktail.escape('liquor') %>">Browse <%= randomCocktail.escape('liquor') %> drinks</a></p>
    <% }; %>
  </div>
</div>

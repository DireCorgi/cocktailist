<form>
  <label for="rating">Rating</label>
    <% for(var i = 1; i<=5; i++){ %>
      <label class="radio">
      <input type="radio" name="rating[rating_num]" value="<%= i %>"> <%= i %>
      </label>
    <% }; %>

  <label for="body">Review</label>
  <textarea name="rating[body]"></textarea>
  <ul id="errors"></ul>
  <button class="submitRating">submit</button>
</form>
